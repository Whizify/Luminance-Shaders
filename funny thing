local function FHWQF_fake_script() -- ShaderGui.LocalScript 
	local script = Instance.new('LocalScript', ShaderGui)

	local frame1 = script.Parent.MenuSelect -- path to the frame
	local frame2 = script.Parent.About --
	local frame3 = script.Parent.Settings --
	local frame4 = script.Parent.Preferences
	local mouse = game:GetService('Players').LocalPlayer:GetMouse(); -- we need the mouse object, which also has the keyboard
	local visible = true
	mouse.KeyDown:connect(function(key) -- whenever a key is pushed down
		key = key:lower() 
		if key == 'p' and visible==true then -- if key pressed is b
			frame1.Visible = false
			frame2.Visible = false
			frame3.Visible = false
			frame4.Visible = false
			visible=false
		elseif key == 'p' and visible==false then
		
			frame1.Visible = true
			frame2.Visible = true
			frame3.Visible = true
			frame4.Visible = true
			visible=true
		end
	end)
end
coroutine.wrap(FHWQF_fake_script)()
local function ELETXF_fake_script() -- ShaderGui.Script 
	local script = Instance.new('Script', ShaderGui)

	Bloomer = .1 -- how much the light reflects on white objects
	game.Lighting:ClearAllChildren()
	game.Workspace.Terrain:ClearAllChildren()
	
	Bloom = Instance.new("BloomEffect", game.Lighting)
	skey = Instance.new("Sky", game.Lighting)
	
	Bloom.Intensity = Bloomer
	print ("bloom set to the preferred anount")
	Bloom.Size = 24
	Bloom.Threshold = 2
	
	cloud = Instance.new("Clouds", game.Workspace.Terrain)
	cloud.Cover=.6
	cloud.Density=1
	cloud.Enabled=false
	
	dep=Instance.new("DepthOfFieldEffect", game.Lighting)
	dep.Enabled=false
	dep.NearIntensity=0
	
	thing=Instance.new("SunRaysEffect", game.Lighting)
	thing.Enabled=false
	
	colorcor=Instance.new("ColorCorrectionEffect", game.Lighting)
	
	game.Lighting.TimeOfDay = ("8:00:00")
	game.Lighting.Ambient = (Color3.fromRGB(70, 70, 70))
	game.Lighting.Brightness = 3
	game.Lighting.ColorShift_Top= (Color3.fromRGB(0, 0, 0))
	game.Lighting.ColorShift_Bottom= (Color3.fromRGB(0, 0, 0))
	game.Lighting.EnvironmentDiffuseScale=1
	game.Lighting.EnvironmentSpecularScale=1
	game.Lighting.GlobalShadows= true
	game.Lighting.OutdoorAmbient= Color3.fromRGB(127, 127, 127)
	game.Lighting.ShadowSoftness=0
	game.Lighting.GeographicLatitude=41
	game.Lighting.ExposureCompensation=0
	game.Lighting.FogColor=Color3.fromRGB(185, 185, 185)
	game.Lighting.FogEnd = 6000
	game.Lighting.FogStart=0
	game.Workspace.Terrain.WaterTransparency=1
	game.Workspace.Terrain.WaterReflectance=1
	game.Workspace.Terrain.WaterWaveSize=1
	
	print("Shaders Loaded Successfully")
end
coroutine.wrap(ELETXF_fake_script)()
local function EPJFVA_fake_script() -- Setting3.LocalScript 
	local script = Instance.new('LocalScript', Setting3)

	while true do
		colore1=script.Parent.Color1.Text
		colore2=script.Parent.Color2.Text
		colore3=script.Parent.Color3aaa.Text
		game.Lighting.FogColor=Color3.fromRGB(tonumber(colore1), tonumber(colore2), tonumber(colore3))
		wait(.1)
	end
end
coroutine.wrap(EPJFVA_fake_script)()
