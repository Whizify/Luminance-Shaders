local function YARP_fake_script() -- XButton_3.LocalScript 
	local script = Instance.new('LocalScript', XButton_3)

	Checked = true
	
	script.Parent.MouseButton1Click:Connect(function()
		if Checked==true then
			Checked=false
			script.Parent.Text="-"
			sethiddenproperty(game:GetService("Workspace").Terrain, "Decoration", false)
	
			local ter = workspace.Terrain
	
			local config = {
				Terrain = true
			}
		else
			Checked=true
			script.Parent.Text="X"
			sethiddenproperty(game:GetService("Workspace").Terrain, "Decoration", true)
	
			local ter = workspace.Terrain
	
			local config = {
				Terrain = true
			}
		end
	end)
end
