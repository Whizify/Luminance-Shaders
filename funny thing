-- Scripts:

local function MMMQEQ_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.About.Visible=false
		script.Parent.Parent.Parent.Settings.Visible=true
		script.Parent.Parent.Parent.Parent.visible=true
	end)
end
coroutine.wrap(MMMQEQ_fake_script)()
local function EULQVN_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.About.Visible=true
		script.Parent.Parent.Parent.Settings.Visible=false
		script.Parent.Parent.Parent.Preferences=false
	end)
end
coroutine.wrap(EULQVN_fake_script)()
local function JWUJGRV_fake_script() -- XButton.LocalScript 
	local script = Instance.new('LocalScript', XButton)

	Checked = true
	preference=script.Parent.Parent.Parent.Preferences
	script.Parent.MouseButton1Click:Connect(function()
		if Checked==true then
			Checked=false
			script.Parent.Text="-"
			game.Lighting.Bloom.Enabled=false
		else
			Checked=true
			script.Parent.Text="X"
			game.Lighting.Bloom.Enabled=true
		end
	end)
end
coroutine.wrap(JWUJGRV_fake_script)()
local function NFXCK_fake_script() -- Bloom.Script 
	local script = Instance.new('Script', Bloom)

	preference=script.Parent.Parent.Parent.Preferences
	script.Parent.MouseButton1Click:Connect(function()
		for _, child in ipairs(script.Parent.Parent.Parent.Preferences:GetChildren()) do
			child.Visible = false
		end
		preference.BloomSettings.Visible=true
	end)
end
