local function CPDYC_fake_script() -- SunRaysSettings.LocalScriptr 
	local script = Instance.new('LocalScript', SunRaysSettings)

	local mouse = game:GetService('Players').LocalPlayer:GetMouse(); -- we need the mouse object, which also has the keyboard
	
	mouse.KeyDown:connect(function(key) -- whenever a key is pushed down
		key = key:lower() 
		if key == '=' or key == '+' then -- if key pressed is b
			script.Parent.Setting2.TextBox.Text=tonumber(script.Parent.Setting2.TextBox.Text+5)
		elseif key == '-' then
			script.Parent.Setting2.TextBox.Text=tonumber(script.Parent.Setting2.TextBox.Text-5)
		end
	end)
end
coroutine.wrap(CPDYC_fake_script)()
